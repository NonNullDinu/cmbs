{
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.checkOnSave.enable": true,
    // discord
    "discord.enabled": true,
    "discord.smallImage": "{app_name}",
    "discord.lowerDetailsNoWorkspaceFound": "No workspace",
    // editing
    "discord.largeImage": "Editing a {LANG} file",
    "discord.detailsEditing": "Editing {file_name}",
    "discord.lowerDetailsEditing": "{workspace}({current_line}/{total_lines})",
    // idling
    "discord.largeImageIdling": "Idling",
    "discord.detailsIdling": "Idling",
    "discord.lowerDetailsIdling": "Workspace: {workspace}",
    // debugging
    "discord.detailsDebugging": "Smashing my head against the wall because of {file_name}",
    "discord.lowerDetailsDebugging": "Debugging: {workspace}",
    // file header
    "copyrighter.author": "Dinu Blanovschi",
    "copyrighter.note": "Licensed under the terms of the BSD-3 Clause license, see LICENSE for more.",
    "copyrighter.newFilesOnly": false,
}